{
	"name": "@lowcode-yunti/frontend-kit",
	"version": "0.0.1-alpha.0",
	"description": "",
	"private": false,
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./common": {
			"types": "./dist/common/index.d.ts",
			"import": "./dist/common/index.mjs",
			"require": "./dist/common/index.cjs"
		},
		"./knowledge": {
			"types": "./dist/knowledge/index.d.ts",
			"import": "./dist/knowledge/index.mjs",
			"require": "./dist/knowledge/index.cjs"
		},
		"./kit": {
			"types": "./dist/kit/index.d.ts",
			"import": "./dist/kit/index.mjs",
			"require": "./dist/kit/index.cjs"
		}
	},
	"scripts": {
		"build": "pnpm --filter @lowcode-yunti/common build && pnpm --filter @lowcode-yunti/knowledge build && pnpm --filter @lowcode-yunti/kit build && unbuild",
		"dev": "pnpm -r --filter='!playground' --filter='!@lowcode-yunti/demo-pkg' dev",
		"studio": "pnpm --filter playground dev",
		"clean": "rimraf dist node_modules packages/*/dist packages/*/node_modules apps/*/dist apps/*/node_modules",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --apply",
		"format": "biome format .",
		"format:fix": "biome format . --write",
		"check": "biome check .",
		"check:fix": "biome check . --apply",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5",
		"@lowcode-yunti/common": "workspace:*",
		"@lowcode-yunti/kit": "workspace:*",
		"@lowcode-yunti/knowledge": "workspace:*",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"axios": "^1.13.2",
		"clsx": "^2.1.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.17.1",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "^19.0.0",
		"@commitlint/config-conventional": "^19.0.0",
		"@types/node": "^20.0.0",
		"husky": "^9.0.0",
		"lint-staged": "^15.0.0",
		"rimraf": "^5.0.0",
		"typescript": "^5.9.3",
		"unbuild": "^3.6.1"
	}
}
